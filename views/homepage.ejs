<%- include("partials/header");  %> 
<%- include("partials/navbar");  %> 
<!-- Musica destacada -->
<!-- Carousel Container -->
<div
  id="carouselMusicaDestacada"
  class="carousel slide carousel-fade"
  data-mdb-ride="carousel"
>
  <!-- Indicadores -->
  <div class="carousel-indicators">
    <button
      type="button"
      data-mdb-target="#carouselMusicaDestacada"
      data-mdb-slide-to="0"
      class="active"
      aria-current="true"
      aria-label="Slide 1"
    ></button>
    <button
      type="button"
      data-mdb-target="#carouselMusicaDestacada"
      data-mdb-slide-to="1"
      aria-label="Slide 2"
    ></button>
    <button
      type="button"
      data-mdb-target="#carouselMusicaDestacada"
      data-mdb-slide-to="2"
      aria-label="Slide 3"
    ></button>
  </div>

  <!-- Parte Interna -->
  <div class="carousel-inner">
    <!-- Imagen -->
    <div class="carousel-item active">
      <img
        src="/images/jcole-album-banner.jpg"
        class="d-block w-100"
        alt="..."
      />
      <div class="carousel-caption d-none d-md-block">
        <h5>Escucha The Off-Season</h5>
        <p>Lo nuevo de J Cole.</p>
      </div>
    </div>

    <!-- Imagen -->
    <div class="carousel-item">
        <div class="carousel-caption d-none d-md-block">
           
          </div>
      <img
        src="/images/kidcudi-motmIII.jpg"
        class="d-block w-100"
        alt="..."
      />
      <div class="carousel-caption d-none d-md-block">
        <h5>Man On the Moon III</h5>
        <p>Ya disponible en Music <i class="fa fa-plus" aria-hidden="true"></i> .</p>
      </div>
    </div>

    <!-- Imagen -->
    <div class="carousel-item">
      <img
        src="/images/the-weeknd-after-hours.jpg"
        class="d-block w-100"
        alt="..."
      />
      <div class="carousel-caption d-none d-md-block text-end">
        <h5>After Hours de The Weeknd</h5>
        <p>Escuchalo ahora, en Music <i class="fa fa-plus" aria-hidden="true"></i>.</p>
      </div>
    </div>
  </div>
  <!-- Parte Interna -->

  <!-- Controles -->
  <button
    class="carousel-control-prev"
    type="button"
    data-mdb-target="#carouselMusicaDestacada"
    data-mdb-slide="prev"
  >
    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
    <span class="visually-hidden">Previous</span>
  </button>
  <button
    class="carousel-control-next"
    type="button"
    data-mdb-target="#carouselMusicaDestacada"
    data-mdb-slide="next"
  >
    <span class="carousel-control-next-icon" aria-hidden="true"></span>
    <span class="visually-hidden">Next</span>
  </button>
</div>
<!-- Container Carousel -->

<section id="content-section" class="cuerpoInicio">
<div class="container">
    <h3>Ultimos lanzamientos</h3>
    <div class="row">
        <div class="col-xl-2 col-4">
            <img class="img-fluid" src="https://t2.genius.com/unsafe/725x0/https%3A%2F%2Fimages.genius.com%2F07a3deb92bfe7c78f95add89edc0e5ed.1000x1000x1.png" alt="" srcset="">
        </div>
    </div>
</div>


</section>


